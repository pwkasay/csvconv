        <td><%= beer.fax %></td>
        <td><%= beer.shipping_address_name %></td>
        <td><%= beer.shipping_address_company_name %></td>
        <td><%= beer.shipping_address_line_1 %></td>
        <td><%= beer.shipping_address_line_2 %></td>
        <td><%= beer.shipping_address_suburb %></td>
