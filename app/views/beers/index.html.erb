        <td><%= beer.billing_address_company_name %></td>
        <td><%= beer.billing_address_line_1 %></td>
        <td><%= beer.billing_address_line_2 %></td>
        <td><%= beer.billing_address_suburb %></td>
        <td><%= beer.billing_address_city %></td>
        <td><%= beer.billing_address_state %></td>
        <td><%= beer.billing_address_country %></td>
        <td><%= beer.billing_address_zipcode %></td>
        <td><%= beer.billing_address_email %></td>
        <td><%= beer.billing_address_phone_number%></td>
        <td><%= beer.order_reference %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<div>
<h4>Read my beer!</h4>
  <%= form_tag import_beers_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import CSV" %>
  <% end %>
</div>
